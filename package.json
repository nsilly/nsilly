{
  "name": "nsilly",
  "version": "0.0.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "ENV=production node ./bin/www",
    "dev": "./node_modules/.bin/nodemon ./node_modules/.bin/babel-node ./bin/www",
    "test": "./node_modules/.bin/jest --forceExit --detectOpenHandles",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "rollback": "./node_modules/.bin/sequelize db:migrate:undo",
    "seed": "./node_modules/.bin/sequelize db:seed:all",
    "build": "rimraf dist/ && mv command command.js && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,error_code.md,README.md,app.test.js --copy-files && mv ./dist/command.js ./dist/command && mv command.js command",
    "command": "./node_modules/.bin/babel-node command"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@nsilly/auth": "^2.1.8",
    "@nsilly/container": "^3.0.2",
    "@nsilly/exceptions": "^3.0.3",
    "@nsilly/log": "^3.0.0",
    "@nsilly/repository": "^3.0.1",
    "@nsilly/response": "^3.0.0",
    "@nsilly/support": "^4.0.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.18.2",
    "commander": "^2.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.15.5",
    "fs-extra": "^5.0.0",
    "hbs": "~4.0.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "mysql2": "^1.5.1",
    "pg": "^7.4.1",
    "redis": "^2.8.0",
    "rimraf": "^2.6.2",
    "rotating-file-stream": "^1.3.9",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^5.1.0",
    "serve-favicon": "~2.4.5",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.0.0",
    "nodemon": "^1.12.5",
    "rimraf": "^2.6.2"
  }
}
